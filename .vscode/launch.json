{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
      {
        "name": "npm dev",
        "request": "launch",
        "runtimeArgs": [
          "run-script",
          "dev"
        ],
        "runtimeExecutable": "npm",
        "skipFiles": [
          "<node_internals>/**"
        ],
        "type": "pwa-node",
      },
      {
        "name": "npm build",
        "request": "launch",
        "runtimeArgs": [
          "run-script",
          "build"
        ],
        "runtimeExecutable": "npm",
        "skipFiles": [
          "<node_internals>/**"
        ],
        "type": "pwa-node",
      },
      {
        "type": "node",
        "request": "launch",
        "name": "Backend",
        "skipFiles": [
            "<node_internals>/**"
        ],
        "cwd": "${workspaceFolder}/backend",
        "program": "${workspaceFolder}/backend/backend.js"
      }
    ]
}
